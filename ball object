public class Ball extends GameObject {
	
	//speed is de valsnelheid van de bal oftewel hoe snel de bal omlaag over het scherm gaat
	private int speed = 0;
	
	public Ball(int x, int y) {
		super();
		setPosition(x, y);
	}
	
	public void setSpeed(int newSpeed) {
		this.speed = newSpeed;
	}
	
	public int getSpeed() {
		return this.speed;
	}
	
	
	@Override
	public String getImageId() {
		return null;
	}

	@Override
	public void onTouched(GameBoard gameBoard) {
	}
}
